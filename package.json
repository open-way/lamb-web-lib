  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4111",
    "build": "ng build",
    "build-lamb-web-lib": "ng build --prod lamb-web-lib && npm run build-lamb-web-lib-styles && npm run build-lamb-web-lib-assets",
