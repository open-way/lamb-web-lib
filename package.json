    "build-lamb-web-lib-assets": "cpx \"./src/assets/**/*\" \"./dist/lamb-web-lib/assets\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
